<h1 class="jumbotron" align="center">
  Upload a File
</h1>
<div class="container" align="center">

  <%= form_tag(:controller => :editor, :action => :fileupload, :multipart => true)  do  %>
    <%= file_field( :post, :attached, accept: 'text/html', class: "btn btn-secondary") %>
    <%= submit_tag "Upload", class: 'btn btn-info' %>
  <% end %>
</div>
